.tbg {
  box-shadow: 0 1px 10px #aaa;
  width: 100%;
  height: 80rpx;
  left: 0;
  background-color: white;
  bottom: -1100rpx;
  position: fixed;
  transition: 0.8s;
  z-index: 200;
}

.tbgshow {
  width: 100%;
  height: 800rpx;
  background-color: white;
  bottom: 0rpx;
  position: fixed;
  transition: 0.8s;
  z-index: 200;
}

.box image {
  width: 200rpx;
  height: 200rpx;
  background-color: white;
  position: absolute;
  margin-top: -80rpx;
  margin-left: 30rpx;
  border: 1rpx solid #ddd;
  border-radius: 10rpx;
  padding: 5rpx;
}

.goodxx {
  font-size: 25rpx;
  position: absolute;
  margin-left: 250rpx;
  margin-top: 20rpx;
  width: 55%;
}

.goodxx .t1 {
  color: orange;
