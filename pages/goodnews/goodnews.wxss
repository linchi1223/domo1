  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.box1 {
  /* background-color: greenyellow; */
  position: fixed;
  border-top: 1rpx solid #f1f1f1;
  box-shadow: 0 0 10rpx #fefefe inset;
