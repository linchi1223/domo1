  transition: 0.8s;
}

.btn11 {
  float: left;
  width: 46%;
  line-height: 75rpx;
  text-align: center;
  background: linear-gradient(to right, #fccc0f, #fa9704);
  border-radius: 50rpx 0 0 50rpx;
  margin: 0 0 0 20rpx;
}

.btn110 {
  float: left;
  width: 96%;
  line-height: 75rpx;
  text-align: center;
  background: linear-gradient(to right, #fccc0f, #fa9704, #fccc0f);
  border-radius: 50rpx;
  margin: 0 auto;
}

.btn21 {
  float: left;
  width: 46%;
  line-height: 75rpx;
  text-align: center;
  background: linear-gradient(to right, #fe7905, #fe4b03);
  border-radius: 0 50rpx 50rpx 0;
  margin: 0 20rpx 0 0;
}

.bodyLock {
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: fixed;
}

.cancel {
  float: right;
  height: 50rpx;
  width: 50rpx;
  margin-right: 30rpx;
  margin-top: 20rpx;
  text-align: center;
  line-height: 50rpx;
  border: 1rpx solid rgb(197, 197, 197);
  border-radius: 50%;
}
