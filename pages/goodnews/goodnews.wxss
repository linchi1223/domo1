/* pages/goodnews/goodnews.wxss */

page {
  min-height: 100%;
  background-color: #f0f3fc;
}

view {
  overflow: visible;
}

.no-scroll {
  height: 100%;
  overflow-y: hidden;
}

.img {
  background-color: #fff;
  width: 100%;
  height: 700rpx;
}

.img image {
  width: 100%;
  height: 100%;
}

.btnfx {
  width: 100%;
  height: 200rpx;
  background-color: #feb351;
}

.jbxx {
  height: 180rpx;
  width: 100%;
  background-color: #fff;
  position: relative;
}

.share-btn {
  border: none;
  background: none;
  border-radius: 0;
  box-shadow: none;
  display: inline-block;
  font-size: 8pt;
  width: 100%;
  margin-top: 50rpx;
}

.share-btn image {
  width: 30rpx;
  height: 30rpx;
  margin: 0 auto;
  display: block;
}

.share-btn .fx {
  font-size: 25rpx;
  display: block;
  text-align: center;
  margin: 0 auto;
}

.jbxx text {
  margin-left: 20rpx;
}

.jbxx .xj {
  color: #ff4d00;
  font-size: 50rpx;
}

.jbxx .xx {
  font-size: 25rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: black;
}

.jbxx .yj {
  font-size: 25rpx;
}

.jbxx .kd {
  font-size: 25rpx;
  position: absolute;
  bottom: 10rpx;
}

.ggxz {
  margin: 15rpx 0;
  background-color: #fff;
  height: 100rpx;
  width: 100%;
  clear: both;
}

.xz {
  line-height: 100rpx;
  margin-left: 20rpx;
  font-size: 25rpx;
  float: left;
}

.gg {
  line-height: 100rpx;
  margin-left: 20rpx;
  font-size: 25rpx;
  float: left;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: black;
}

.jt image {
  display: block;
  float: right;
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
  margin-top: 35rpx;
}

/* 下面底部 */

.jiesuan-box {
  z-index: 100;
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  border-bottom: 1px solid #eee;
  border-top: 1px solid #eee;
  background-color: #fff;
}

.jiesuan-box .btns1 {
  width: 40%;
  font-size: 20rpx;
  text-align: center;
  /* background-color: green; */
}

.jiesuan-box .btns1 .btn {
  margin-top: 20rpx;
  float: left;
  width: 33%;
}

.jiesuan-box .btns2 {
  width: 60%;
  text-align: center;
  color: white;
  font-size: 30rpx;
}

.jiesuan-box .btns2 .btn1 {
  float: left;
  width: 50%;
  line-height: 100rpx;
  background-color: #feb351;
}

.jiesuan-box .btns1 .btn image {
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
  display: block;
}

.jiesuan-box .btns2 .btn2 {
  float: left;
  width: 50%;
  line-height: 100rpx;
  background-color: #e43e2e;
}

.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid #777;
  text-align: center;
  line-height: 80rpx;
  background-color: white;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 50%;
  color: #777;
  background-color: white;
}

.on {
  color: #ff4d00;
  border-bottom: 3rpx solid #ff4d00;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
  background-color: white;
  z-index: 50;
}

.swiper-box view {
  text-align: center;
  background-color: white;
}

