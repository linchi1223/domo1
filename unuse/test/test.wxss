.zbg .tbg {
  box-shadow: 0 1px 10px #aaa;
  width: 100%;
  height: 900rpx;
  left: 0;
  background-color: white;
  bottom: -1100rpx;
  position: fixed;
  transition: 1s;
  
}

.zbg .tbgshow {
  width: 100%;
  height: 900rpx;
  background-color: white;
  bottom: 0rpx;
  position: fixed;
  transition: 1s;
  
}

.zbg image {
  width: 200rpx;
  height: 200rpx;
  background-color: white;
  position: absolute;
  margin-top: -80rpx;
  margin-left: 30rpx;
  border: 1rpx solid #ddd;
  border-radius: 10rpx;
  padding: 5rpx;
}

.zbg  .goodxx {
  font-size: 25rpx;
  position: absolute;
  margin-left: 250rpx;
  margin-top: 20rpx;
}

.goodxx .t1 {
  color: orange;
  font-size: 35rpx;
}

.goodxx .t3 {
  color: black;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.box1 {
  /* background-color: greenyellow; */
