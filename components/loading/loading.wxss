.loading {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  height: 210rpx;
  border-radius: 10rpx;
  background: rgba(0, 0, 0, 0.5);
}

.loading .icon {
  width: 64rpx;
  height: 64rpx;
}

.loading .title {
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #fff;
}