.star {
  display: flex;
  align-items: center;
}

.star .icon-star {
  margin-right: 14rpx;
  width: 32rpx;
  height: 32rpx;
}

.star .score {
  margin-left: 10rpx;
  font-size: 26rpx;
  color: #333;
}

.star .icon-star .item{
  position: relative;
  width: 32rpx;
  height: 32rpx;
  top: -30rpx;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
}
.star .icon-star .item .left{
  flex: 1;
  height: 32rpx;
}
.star .icon-star .item .right{
  flex: 1;
  height: 32rpx;
}